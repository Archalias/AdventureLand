{
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "semi": "warn",
    "prefer-destructuring": [
      "error",
      {
        "array": true,
        "object": true
      },
      {
        "enforceForRenamedProperties": false
      }
    ]
  },
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": false
    }
  },
  "plugins": [
    "jest"
  ],
  "env": {
    "jest/globals": true,
    "jest": true
  }
}